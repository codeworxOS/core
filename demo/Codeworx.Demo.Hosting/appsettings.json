{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Hosting": {
    "FeatureAssemblies": {
      "Codeworx.AspNetCore": true
    },
    "Plugins": {
      "Codeworx.Demo.PluginOne": 1
    }
  },
  "Authentication": {
    "Schemes": {
      "Introspection": {
        "Authority": "https://login.microsoftonline.com/common",
        "ValidAudiences": [ "{AUDIENCE}" ],
        "ValidIssuer": "{DOMAIN}"
      }
    }
  }
}
